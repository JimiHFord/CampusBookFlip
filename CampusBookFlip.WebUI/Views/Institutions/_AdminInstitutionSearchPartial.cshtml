@model CampusBookFlip.WebUI.Models.CollegeSearchResultsViewModel

<div class="row">
    <strong>Count:</strong> @Model.Colleges.Count() <strong>Milliseconds:</strong> @Model.TotalMilliseconds <strong>Seconds:</strong> @Model.TotalSeconds
</div>
<table>
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.Colleges.Name)*@Name
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Colleges.Address)*@Address
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Colleges.City)*@City
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Colleges.State)*@State
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Colleges.Zip)*@Zip
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Colleges.Phone)*@Phone
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Colleges.WebAddress)*@Web Address
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Colleges.Activated)*@Activated
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Colleges.DateActivated)*@Date Activated
        </th>
        @*<th></th>*@
    </tr>

@foreach (var item in Model.Colleges) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Zip)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WebAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Activated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateActivated)
        </td>
        @*<td>
            @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
        </td>*@
    </tr>
}

</table>
