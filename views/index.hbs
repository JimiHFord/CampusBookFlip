<!-- Amazon Modal 1 -->
<div class="custom-front-page-modal modal fade" id="amazonComparison" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-content">
    <div class="close-modal" data-dismiss="modal">
